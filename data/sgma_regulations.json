  "transfer_rules": [
    {
      "id": "rule_intra_basin",
      "name": "Intra-basin Transfer",
      "description": "Transfers within the same basin are generally allowed without additional permits. The GSA must be notified of transfers exceeding 10 AF.",
      "source": "SGMA Section 10726.4",
      "conditions": ["same_basin"],
      "requires_permit": false,
      "notification_threshold_af": 10
    },
    {
      "id": "rule_inter_basin_adjacent",
      "name": "Inter-basin Transfer (Adjacent)",
      "description": "Transfers between adjacent basins require GSA notification but no permit if under 100 AF annually. Both GSAs must be notified.",
      "source": "SGMA Section 10726.4(a)(2)",
      "conditions": ["adjacent_basin", "quantity_under_100af"],
      "requires_permit": false,
      "requires_notification": true,
      "notification_parties": ["source_gsa", "destination_gsa"]
    },
    {
      "id": "rule_inter_basin_permit",
      "name": "Inter-basin Transfer (Permit Required)",
      "description": "Large inter-basin transfers (>100 AF annually) require permit approval from both GSAs. Environmental review may be required under CEQA.",
      "source": "SGMA Section 10726.4(b)",
      "conditions": ["different_basin", "quantity_over_100af"],
      "requires_permit": true,
      "permit_process": "Joint GSA review, 45-day public comment period",
      "estimated_timeline_days": 90
    },
    {
      "id": "rule_critically_overdrafted",
      "name": "Critically Overdrafted Basin Restriction",
      "description": "Transfers OUT of critically overdrafted basins may be restricted during drought conditions or when basin levels are below minimum thresholds.",
      "source": "SGMA Section 10720.7",
      "conditions": ["source_critically_overdrafted", "drought_conditions"],
      "requires_permit": true,
      "may_be_denied": true,
      "appeal_process": "State Water Resources Control Board"
    },
    {
      "id": "rule_seasonal_ag",
      "name": "Agricultural Season Priority",
      "description": "During peak agricultural season (April through September), local agricultural use has priority over transfers. Emergency transfers for crop preservation may be expedited.",
      "source": "Local GSA Policy",
      "conditions": ["agricultural_season"],
      "priority_to": "local_agriculture",
      "season_months": [4, 5, 6, 7, 8, 9],
      "emergency_provisions": true
    },
    {
      "id": "rule_minimum_allocation",
      "name": "Minimum Allocation Preservation",
      "description": "Farmers must maintain a minimum of 20% of their annual allocation as reserve. Transfers that would reduce balance below this threshold require special approval.",
      "source": "Local GSA Policy",
      "conditions": ["balance_below_minimum"],
      "minimum_reserve_percent": 20,
      "requires_permit": true
    },
    {
      "id": "rule_new_well_restriction",
      "name": "New Well Restriction Zone",
      "description": "In designated restriction zones, new wells require GSA approval and existing allocations may be reduced to achieve basin sustainability.",
      "source": "SGMA Section 10726.4(c)",
      "conditions": ["restriction_zone"],
      "requires_permit": true,
      "allocation_reduction_possible": true
    }
  ],
